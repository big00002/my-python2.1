คะแนน = float(input("รับค่าคะแนน: "))
if 80 <= คะแนน <= 100:
    print("เกรด A")
elif 70 <= คะแนน <= 79:
    print("เกรด B")
elif 60 <= คะแนน <= 69:
    print("เกรด C")
elif 50 <= คะแนน <= 59:
    print("เกรด D")
elif 40 <= คะแนน <= 49:
    print("เกรด E")
elif 0 <= คะแนน < 40:
    print("เกรด F")
else:
    print("กรุณาป้อนคะแนนให้ถูกต้อง")